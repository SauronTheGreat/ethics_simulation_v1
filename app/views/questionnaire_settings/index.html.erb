<h1>Listing questionnaire_settings</h1>



<table width="85%" style="margin: auto">
  <tr>
	<th>Questionnaire</th>
	<th>Category</th>
	<th>Value</th>


	<td width="10%"></td>

  </tr>
  <% @questionnaire_settings.each do |questionnaire_setting| %>
	<tr>


	  <td style="text-align: left;border: solid 1px #000000"><%= Questionnaire.find(questionnaire_setting.questionnaire_id).name %></td>
	  <td style="text-align: left;border: solid 1px #000000"><%= Category.find(questionnaire_setting.category_id).name %></td>
	  <td style="text-align: left;border: solid 1px #000000"><%= questionnaire_setting.value %></td>


	  <td width="10%"></td>
	  <td class="blacky"><%= link_to image_tag('/images/menu/settings_icon_off.png', :style=>"width:50px;height:50px;", :class=>'rollover', :title=>'Manage'), questionnaire_setting %></td>
	  <td class="blacky"><%= link_to image_tag('/images/menu/rename_icon_off.png', :style=>"width:40px;height:40px;", :class=>'rollover', :title=>'Rename'), edit_questionnaire_setting_path(questionnaire_setting) %></td>
	  <td class="blacky"><%= link_to image_tag('/images/menu/delete-icon_off.png', :style=>"width:45px;height:45px;", :class=>'rollover', :title=>'Delete'), questionnaire_setting, :confirm => 'Are you sure?', :method => :delete %></td>

	</tr>
  <% end %>
</table>

  <td><%= link_to "Back",questionnaire_path(@questionnaire) %></td>